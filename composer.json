    "homepage": "http://github.com/keywan-ghadami-oxid/monoxid",
    "type": "oxideshop-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Keywan Ghadami",
            "email": "keywan.ghadmi@oxid-esales.com",
            "homepage": "http://www.oxid-esales.com"
        }
    ],
    "extra": {
